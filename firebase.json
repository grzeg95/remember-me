{
  "functions": {
    "predeploy": "npm --prefix \"$RESOURCE_DIR\" run build",
    "source": "functions"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "emulators": {
    "functions": {
      "port": "5000",
      "host": "localhost"
    },
    "firestore": {
      "port": 9090,
      "host": "localhost"
    },
    "ui": {
      "enabled": true,
      "host": "localhost"
    },
    "auth": {
      "port": "9099",
      "host": "localhost"
    }
  },
  "remoteconfig": {}
}
