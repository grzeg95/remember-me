    "karma": "~6.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "mini-css-extract-plugin": "^1.3.9",
    "protractor": "~7.0.0",
    "sass-lint": "^1.13.1",
    "sass-loader": "^11.0.1",
    "terser-webpack-plugin": "^5.1.1",
    "ts-node": "~9.1.1",
    "tslib": "^2.1.0",
