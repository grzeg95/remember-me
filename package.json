{
  "name": "remember-me-angular",
  "version": "0.0.0",
  "scripts": {
    "serve:emulators": "firebase use dev && firebase emulators:start --import=./emulators-backup --export-on-exit",
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy:hosting:dev": "ng build --configuration=dev && firebase use dev && firebase deploy --only hosting",
    "deploy:hosting:prod": "ng build --configuration=production && firebase use default && firebase deploy --only hosting",
    "deploy:functions:dev": "npm run deploy:dev --prefix functions",
    "deploy:functions:prod": "npm run deploy:prod --prefix functions"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.1.1",
    "@angular/cdk": "^10.2.0",
    "@angular/common": "^10.1.1",
    "@angular/compiler": "^10.1.1",
    "@angular/core": "^10.1.1",
    "@angular/fire": "^6.0.2",
    "@angular/forms": "^10.1.1",
    "@angular/material": "^10.2.0",
    "@angular/platform-browser": "^10.1.1",
    "@angular/platform-browser-dynamic": "^10.1.1",
    "@angular/router": "^10.1.1",
    "@fortawesome/angular-fontawesome": "^0.7.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@popperjs/core": "^2.2.2",
    "bootstrap": "^4.4.1",
    "deep-equal": "^1.1.1",
    "firebase": "^7.20.0",
    "firebase-admin": "^9.1.1",
    "firebase-functions": "^3.11.0",
    "functions": "file:functions",
    "g": "^2.0.1",
    "jquery": "^3.5.0",
    "mixin-deep": "^2.0.1",
    "ng-connection-service": "^1.0.4",
    "ngx-cookie-service": "^10.0.1",
    "node-sass": "^4.14.1",
    "popper.js": "^1.16.1",
    "rxjs": "^6.5.5",
    "sass-lint": "^1.13.1",
    "sass-loader": "^7.3.1",
    "set-value": "^3.0.2",
    "tslib": "^1.11.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1001.1",
    "@angular/cli": "^9.1.6",
    "@angular/compiler-cli": "^10.1.1",
    "@angular/language-service": "~9.0.7",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.12.35",
    "codelyzer": "^6.0.0",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "protractor": "^5.4.3",
    "ts-node": "~7.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2"
  }
}
