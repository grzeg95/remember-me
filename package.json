{
  "name": "remember-me-angular",
  "version": "0.0.0",
  "scripts": {
    "serve:emulators": "python closeEmulator.py && firebase use dev && firebase emulators:start",
    "ng": "ng",
    "start": "ng serve",
    "build-dev": "ng build --configuration=dev",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy:hosting:dev": "ng build --configuration=dev && firebase use dev && firebase deploy --only hosting",
    "deploy:hosting:prod": "ng build --configuration=production && firebase use default && firebase deploy --only hosting",
    "deploy:functions:dev": "npm run deploy:dev --prefix functions",
    "deploy:functions:prod": "npm run deploy:prod --prefix functions"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.1.0",
    "@angular/cdk": "~14.1.0",
    "@angular/common": "~14.1.0",
    "@angular/compiler": "~14.1.0",
    "@angular/core": "~14.1.0",
    "@angular/fire": "~7.4.1",
    "@angular/forms": "~14.1.0",
    "@angular/material": "~14.1.0",
    "@angular/platform-browser": "~14.1.0",
    "@angular/platform-browser-dynamic": "~14.1.0",
    "@angular/router": "~14.1.0",
    "@fortawesome/angular-fontawesome": "~0.11.1",
    "@fortawesome/fontawesome-svg-core": "~6.1.1",
    "@fortawesome/free-brands-svg-icons": "~6.1.1",
    "@fortawesome/free-regular-svg-icons": "~6.1.1",
    "@fortawesome/free-solid-svg-icons": "~6.1.1",
    "@popperjs/core": "~2.11.5",
    "bootstrap": "~5.2.0",
    "buffer": "^6.0.3",
    "firebase": "~9.9.1",
    "firebase-functions": "~3.22.0",
    "jquery": "~3.6.0",
    "material-design-icons": "~3.0.1",
    "rxjs": "~7.5.6",
    "sass": "~1.54.0",
    "set-value": "~4.1.0",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.1.0",
    "@angular/cli": "~14.1.0",
    "@angular/compiler-cli": "~14.1.0",
    "@types/node": "^16.11.24",
    "firebase-tools": "~11.3.0",
    "sass-lint": "~1.13.1",
    "typescript": "~4.7.4"
  }
}
