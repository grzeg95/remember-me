{
  "name": "remember-me-angular",
  "version": "0.0.0",
  "scripts": {
    "serve:emulators": "firebase use dev && firebase emulators:start --import=./emulators-backup --export-on-exit",
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "build-dev": "ng build --configuration=dev",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy:hosting:dev": "ng build --configuration=dev && firebase use dev && firebase deploy --only hosting",
    "deploy:hosting:prod": "ng build --configuration=production && firebase use default && firebase deploy --only hosting",
    "deploy:functions:dev": "npm run deploy:dev --prefix functions",
    "deploy:functions:prod": "npm run deploy:prod --prefix functions"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.7",
    "@angular/cdk": "~11.2.6",
    "@angular/common": "~11.2.7",
    "@angular/compiler": "~11.2.7",
    "@angular/core": "~11.2.7",
    "@angular/fire": "~6.1.4",
    "@angular/forms": "~11.2.7",
    "@angular/material": "~11.2.6",
    "@angular/platform-browser": "~11.2.7",
    "@angular/platform-browser-dynamic": "~11.2.7",
    "@angular/router": "~11.2.7",
    "@fortawesome/angular-fontawesome": "~0.8.2",
    "@fortawesome/fontawesome-svg-core": "~1.2.35",
    "@fortawesome/free-brands-svg-icons": "~5.15.3",
    "@fortawesome/free-regular-svg-icons": "~5.15.3",
    "@fortawesome/free-solid-svg-icons": "~5.15.3",
    "@popperjs/core": "~2.9.1",
    "bootstrap": "~4.6.0",
    "firebase": "~8.3.1",
    "firebase-admin": "~9.5.0",
    "firebase-functions": "~3.13.2",
    "jquery": "~3.6.0",
    "material-design-icons": "~3.0.1",
    "ngx-cookie-service": "~11.0.2",
    "popper.js": "~1.16.1",
    "rxjs": "~6.6.6",
    "sass": "~1.32.8",
    "set-value": "~3.0.2",
    "tslib": "~2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.6",
    "@angular/cli": "~11.2.6",
    "@angular/compiler-cli": "~11.2.7",
    "@angular/language-service": "~11.2.7",
    "@types/jasmine": "~3.6.7",
    "@types/node": "~14.14.36",
    "codelyzer": "~6.0.1",
    "firebase-tools": "~9.7.0",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.3.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "~1.5.4",
    "protractor": "~7.0.0",
    "sass-lint": "~1.13.1",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.3",
    "typescript": "~4.1.0"
  }
}
