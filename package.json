{
  "name": "remember-me-angular",
  "version": "0.0.0",
  "scripts": {
    "serve:emulators": "python closeEmulator.py && firebase use dev && firebase emulators:start",
    "ng": "ng",
    "start": "ng serve",
    "build-dev": "ng build --configuration=dev",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy:hosting:dev": "ng build --configuration=dev && firebase use dev && firebase deploy --only hosting",
    "deploy:hosting:prod": "ng build --configuration=production && firebase use default && firebase deploy --only hosting",
    "deploy:functions:dev": "npm run deploy:dev --prefix functions",
    "deploy:functions:prod": "npm run deploy:prod --prefix functions"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.2",
    "@angular/cdk": "~13.2.2",
    "@angular/common": "~13.2.2",
    "@angular/compiler": "~13.2.2",
    "@angular/core": "~13.2.2",
    "@angular/fire": "~7.2.1",
    "@angular/forms": "~13.2.2",
    "@angular/material": "~13.2.2",
    "@angular/platform-browser": "~13.2.2",
    "@angular/platform-browser-dynamic": "~13.2.2",
    "@angular/router": "~13.2.2",
    "@fortawesome/angular-fontawesome": "~0.10.1",
    "@fortawesome/fontawesome-svg-core": "~1.3.0",
    "@fortawesome/free-brands-svg-icons": "~6.0.0",
    "@fortawesome/free-regular-svg-icons": "~6.0.0",
    "@fortawesome/free-solid-svg-icons": "~6.0.0",
    "@popperjs/core": "~2.11.2",
    "bootstrap": "~5.1.3",
    "buffer": "^6.0.3",
    "firebase": "~9.6.6",
    "firebase-functions": "~3.18.0",
    "jquery": "~3.6.0",
    "material-design-icons": "~3.0.1",
    "rxjs": "~7.5.4",
    "sass": "~1.49.7",
    "set-value": "~4.1.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.3",
    "@angular/cli": "~13.2.3",
    "@angular/compiler-cli": "~13.2.2",
    "@types/node": "^16.11.24",
    "firebase-tools": "~9.23.3",
    "sass-lint": "~1.13.1",
    "typescript": "~4.5.5"
  }
}
