{
  "name": "remember-me-angular",
  "version": "0.0.0",
  "scripts": {
    "serve:angular": "node scripts/set-env.js && node scripts/set-remote-config-default-env.js && ng serve",
    "serve:emulators": "node scripts/close-parts-of-firebase-emulator.js && firebase use dev && firebase emulators:start",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:hosting:static": "node scripts/set-env.js && node scripts/set-remote-config-default-env.js && ng build --configuration=production && node scripts/index-html-parser.js",
    "build:hosting:image": "cd public && gcloud builds submit --tag gcr.io/remember-me-4/web-site",
    "deploy:hosting:image": "cd public && gcloud run deploy web-site --region=europe-west4 --image gcr.io/remember-me-4/web-site --platform managed",
    "build:functions:dev": "npm run build:dev --prefix functions",
    "deploy:functions": "npm run deploy:prod --prefix functions"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.2.5",
    "@angular/cdk": "16.2.4",
    "@angular/common": "16.2.5",
    "@angular/compiler": "16.2.5",
    "@angular/core": "16.2.5",
    "@angular/fire": "16.0.0",
    "@angular/forms": "16.2.5",
    "@angular/material": "16.2.4",
    "@angular/platform-browser": "16.2.5",
    "@angular/platform-browser-dynamic": "16.2.5",
    "@angular/router": "16.2.5",
    "@fortawesome/angular-fontawesome": "0.13.0",
    "@fortawesome/fontawesome-svg-core": "6.4.2",
    "@fortawesome/free-brands-svg-icons": "6.4.2",
    "@fortawesome/free-regular-svg-icons": "6.4.2",
    "@fortawesome/free-solid-svg-icons": "6.4.2",
    "@popperjs/core": "2.11.8",
    "bootstrap": "5.3.1",
    "buffer": "6.0.3",
    "jquery": "3.7.1",
    "material-design-icons": "3.0.1",
    "rxjs": "7.8.1",
    "sass": "1.67.0",
    "tslib": "2.6.2",
    "zone.js": "0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.2.2",
    "@angular/cli": "16.2.2",
    "@angular/compiler-cli": "16.2.5",
    "@types/node": "16.18.50",
    "cheerio": "1.0.0-rc.12",
    "colors": "1.4.0",
    "dotenv": "16.3.1",
    "firebase-tools": "12.5.4",
    "sass-lint": "1.13.1",
    "typescript": "5.0.4"
  }
}
