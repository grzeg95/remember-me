{
  "name": "remember-me-angular",
  "version": "0.0.0",
  "scripts": {
    "serve:angular": "node scripts/set-env.js && node scripts/set-remote-config-default-env.js && ng serve",
    "serve:emulators": "node scripts/close-parts-of-firebase-emulator.js && firebase use dev && firebase emulators:start",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:hosting:static": "node scripts/set-env.js && node scripts/set-remote-config-default-env.js && ng build --configuration=production && node scripts/index-html-parser.js",
    "build:hosting:image": "cd public && gcloud builds submit --tag gcr.io/remember-me-4/web-site",
    "deploy:hosting:image": "cd public && gcloud run deploy web-site --region=europe-west4 --image gcr.io/remember-me-4/web-site --platform managed",
    "build:functions:dev": "npm run build:dev --prefix functions",
    "deploy:functions": "npm run deploy:prod --prefix functions"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.1.2",
    "@angular/cdk": "15.1.2",
    "@angular/common": "15.1.2",
    "@angular/compiler": "15.1.2",
    "@angular/core": "15.1.2",
    "@angular/forms": "15.1.2",
    "@angular/material": "15.1.2",
    "@angular/platform-browser": "15.1.2",
    "@angular/platform-browser-dynamic": "15.1.2",
    "@angular/router": "15.1.2",
    "@fortawesome/angular-fontawesome": "0.12.1",
    "@fortawesome/fontawesome-svg-core": "6.2.1",
    "@fortawesome/free-brands-svg-icons": "6.2.1",
    "@fortawesome/free-regular-svg-icons": "6.2.1",
    "@fortawesome/free-solid-svg-icons": "6.2.1",
    "@popperjs/core": "2.11.6",
    "bootstrap": "5.2.3",
    "buffer": "6.0.3",
    "firebase": "9.16.0",
    "jquery": "3.6.3",
    "material-design-icons": "3.0.1",
    "rxjs": "7.8.0",
    "sass": "1.57.1",
    "tslib": "2.5.0",
    "zone.js": "0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.1.3",
    "@angular/cli": "15.1.3",
    "@angular/compiler-cli": "15.1.2",
    "@types/node": "16.18.10",
    "cheerio": "1.0.0-rc.12",
    "colors": "1.4.0",
    "dotenv": "16.0.3",
    "firebase-tools": "11.21.0",
    "sass-lint": "1.13.1",
    "typescript": "4.9.4"
  }
}
